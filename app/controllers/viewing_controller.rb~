class ViewingController < ApplicationController
  before_filter :redirect_if_not_logged_in
 
  def newsfeed
  end

  def profile
num = @current_user.id - int(params[:userProfile])
  flash[:error] = "#{num== 0}"
      if @current_user.friendships.include?(User.find_by_id(params[:userProfile])) or @current_user.id == params[:userProfile]
#flash[:error] = "f"
      render :template => "viewing/profileFriend"
    else # Not friends
#     flash[:error]  = "n"
      render :template => "viewing/profileNonFriend"
    end

  end

end
